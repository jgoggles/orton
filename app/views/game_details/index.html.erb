<h1>Listing game_details</h1>

<table>
  <tr>
    <th>Game</th>
    <th>Team</th>
    <th>Is home</th>
    <th>Score</th>
    <th>Spread</th>
    <th>Yards gained</th>
    <th>Yards allowed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @game_details.each do |game_detail| %>
  <tr>
    <td><%= game_detail.game_id %></td>
    <td><%= game_detail.team.nickname %></td>
    <td><%= game_detail.is_home %></td>
    <td><%= game_detail.score %></td>
    <td><%= game_detail.spread %></td>
    <td><%= game_detail.yards_gained %></td>
    <td><%= game_detail.yards_allowed %></td>
    <td><%= link_to 'Show', game_detail %></td>
    <td><%= link_to 'Edit', edit_game_detail_path(game_detail) %></td>
    <td><%= link_to 'Destroy', game_detail, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

